{
    "name": "feedback-system",
    "private": true,
    "type": "module",
    "scripts": {
        "check": "oxlint",
        "check-types": "pnpm -r check-types",
        "format": "pnpm exec biome format --fix --json-formatter-enabled=false",
        "build:server": "pnpm -F server build",
        "build:web": "pnpm -F web build",
        "build:admin": "pnpm -F admin build",
        "dev:admin": "pnpm -F admin dev",
        "start:server": "pnpm -F server start",
        "dev:web": "pnpm -F web dev",
        "dev:server": "pnpm -F server dev",
        "dev:db:reset": "pnpm -F @shared/database dev:db:reset",
        "dev:db:seed": "pnpm -F @shared/database dev:db:seed",
        "dev:db:check": "pnpm -F @shared/database dev:db:check",
        "dev:db:migrate": "pnpm -F @shared/database dev:db:migrate",
        "dev:auth:generate": "pnpm -F @shared/auth dev:auth:generate",
        "dev:auth:migrate": "pnpm -F @shared/auth dev:auth:migrate",
        "dev:auth:seed": "pnpm -F @shared/auth dev:auth:seed",
        "staging:admin": "pnpm -F admin staging",
        "staging:web": "pnpm -F web staging",
        "staging:server": "pnpm -F server staging",
        "staging:db:reset": "pnpm -F @shared/database staging:db:reset",
        "staging:db:seed": "pnpm -F @shared/database staging:db:seed",
        "staging:db:check": "pnpm -F @shared/database staging:db:check",
        "staging:db:migrate": "pnpm -F @shared/database staging:db:migrate",
        "staging:auth:generate": "pnpm -F @shared/auth staging:auth:generate",
        "staging:auth:migrate": "pnpm -F @shared/auth staging:auth:migrate",
        "staging:auth:seed": "pnpm -F @shared/auth staging:auth:seed"
    },
    "devDependencies": {
        "@biomejs/biome": "catalog:",
        "oxlint": "catalog:"
    },
    "packageManager": "pnpm@10.25.0",
    "engines": {
        "node": ">=24.11.0"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "better-sqlite3",
            "esbuild",
            "msgpackr-extract",
            "prisma",
            "protobufjs",
            "sharp"
        ]
    }
}